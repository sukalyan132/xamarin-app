<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             Title="Doctor Forget Password"
             x:Class="CloudCakes.Gotoforgetpassword">
    <StackLayout Padding="20">
        <Entry x:Name="EmailId" Placeholder="Enter Email Id" WidthRequest="150" Keyboard="Email"></Entry>
        <StackLayout Orientation="Horizontal">
            <Button BackgroundColor="Gray" TextColor="White" Text="Reset"/>
            <Button BackgroundColor="Maroon" TextColor="White" Text="Cancel" />
        </StackLayout>
    </StackLayout>
</ContentPage>